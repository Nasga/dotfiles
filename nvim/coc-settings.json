{
  "javascript.suggestionActions.enabled": false,
  "eslint.autoFixOnSave": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "typescript",
    "typescriptreact",
    "javascript",
    "graphql",
    "css",
    "Markdown",
    "json"
  ],
  "list.normalMappings": {
    "<C-c>": "do:exit"
  },
  "list.insertMappings": {
    "<C-c>": "do:exit"
  },
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    },
    "graphql": {
      "command": "graphql-lsp",
      "args": ["server", "-m", "stream"],
      "filetypes": ["typescript", "typescriptreact", "graphql"]
    }
  },
  "sql.database": "postgresql",
  "cSpellExt.enableDictionaries": ["french"],
  "cSpell.language": "en,fr",
  "cSpell.userWords": [
    "ALUR",
    "Aircall",
    "CLOUDCONVERT",
    "Clarkson",
    "Frontapp",
    "ILIKE",
    "Iban",
    "Onboarded",
    "RGDD",
    "SEPA",
    "SSOT",
    "Sepas",
    "Taitbout",
    "Trustpilot",
    "Unicity",
    "anonymizing",
    "apikey",
    "bankin",
    "caticon",
    "conver",
    "creditmemo",
    "datasources",
    "datetime",
    "docupilot",
    "fundcall",
    "hubspot",
    "immo",
    "microservices",
    "middlewares",
    "pdfs",
    "pino",
    "postgresql",
    "quickbooks",
    "rlmc",
    "stepfunction",
    "upsert",
    "xlarge",
    "xsmall",
    "yolo"
  ],
}
